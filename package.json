    "@eslint/js": "^9.27.0",
    "browser-sync": "^3.0.4",
    "concurrently": "^9.1.2",
    "connect-livereload": "^0.6.1",
    "eslint": "^9.27.0",
    "globals": "^16.1.0",
    "prettier": "^3.5.3"
  }
